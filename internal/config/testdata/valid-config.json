{
  "schemaVersion": "1.1.0",
  "connector": {
    "name": "test-connector",
    "version": "1.0.0",
    "description": "A test connector for unit testing",
    "input": {
      "type": "httpPolling",
      "endpoint": "https://api.example.com/data",
      "schedule": "*/5 * * * *",
      "method": "GET"
    },
    "filters": [
      {
        "type": "mapping",
        "mappings": [
          { "source": "id", "target": "externalId" },
          { "source": "name", "target": "displayName" }
        ]
      }
    ],
    "output": {
      "type": "httpRequest",
      "endpoint": "https://api.destination.com/import",
      "method": "POST"
    }
  }
}
